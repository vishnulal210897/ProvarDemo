    <sfActionLink n="Share" t="Share" modes="CombinedAttachments" requiresRow="true" lightningActionName="ShareFile"/>
    <sfActionLink n="PublicLink" t="Public Link" modes="CombinedAttachments" requiresRow="true" lightningActionName="FileLinkShare"/>
    <sfActionLink n="ViewFileDetails" t="View File Details" modes="CombinedAttachments" requiresRow="true" lightningActionName="GetFileDetail"/>
    <sfActionLink n="UploadNewVersion" t="Upload New Version" modes="CombinedAttachments" requiresRow="true" lightningActionName="NewVersion"/>
    <sfActionLink n="EditFileDetails" t="Edit File Details" modes="CombinedAttachments" requiresRow="true"/>
    <sfActionLink n="RemoveFromRecord" t="Remove from Record" modes="CombinedAttachments" requiresRow="true" lightningActionName="DetachFile"/>
